package new2024;



public class ArrayOfObjects {
  public static void main(String[] args) {
     Student  s1=new Student();
     s1.rollNo=1;
     s1.studentName="Abhi";
     s1.marks=88;
     Student  s2=new Student();
     s2.rollNo=2;
     s2.studentName="Abhilash";
     s2.marks=99;
    		 
     Student students[]=new Student[2];
     students[0]=s1;
     students[1]=s2;
     for(int i=0;i<students.length;i++) {
    	 System.out.print(students[i].rollNo+" "+students[i].studentName+" "+students[i].marks);
     }
     
}
}
